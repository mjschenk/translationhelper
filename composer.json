{
    "name": "webthings/translationhelper",
    "description": "Translation Helper for Magento 2, enhanced for broad version compatibility",
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Roman Sliusar",
            "email": "roman.slusar95@gmail.com"
        },
        {
            "name": "Marc Schenk",
            "email": "info@webthin.gs"
        }
    ],
    "require": {
        "php": "~7.1.3 || ~7.2.0 || ~7.3.0 || ~7.4.0 || ^8.0 || ^8.1 || ^8.2",
        "magento/magento2-base": "^2.2.0 || ^2.3.0 || ^2.4.0",
        "magento/framework": "^101.0 || ^102.0 || ^103.0"
    },
    "require-dev": {
        "magento/magento-coding-standard": "^4.0",
        "phpunit/phpunit": "^7.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "WebThings\\TranslationHelper\\": ""
        }
    }
}
